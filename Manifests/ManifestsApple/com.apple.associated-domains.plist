							<key>pfm_macos_min</key>
							<string>11.0</string>
							<key>pfm_name</key>
							<string>EnableDirectDownloads</string>
							<key>pfm_type</key>
							<string>boolean</string>
							<key>pfmx_supported_oses</key>
							<dict>
								<key>macOS</key>
								<dict>
									<key>pfmx_introduced</key>
									<string>11.0</string>
								</dict>
							</dict>
						</dict>
					</array>
					<key>pfm_type</key>
					<string>dictionary</string>
				</dict>
			</array>
